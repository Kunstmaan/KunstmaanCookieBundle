{
    "name": "kunstmaan/cookie-bundle",
    "type": "symfony-bundle",
    "description": "The Kunstmaan Cookie Bundle provides a cookie bar; detailed pop-up window and a similar page explaining each type of cookie used on the website.",
    "keywords": [
        "kunstmaan",
        "cms",
        "gdpr"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~3.4 | ^4.0",
        "doctrine/orm": "^2.2.3",
        "sensio/generator-bundle": "~2.5|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Kunstmaan\\CookieBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Kunstmaan\\CookieBundle\\Composer\\DevelopmentEnvironmentManager::setup"
        ]
    }
}
